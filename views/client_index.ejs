<html>
  <head>
    <title> Clients </title>
    <link href="https://fonts.googleapis.com/css?family=Lato:100" rel="stylesheet">
    <link href="../public/styles.css" rel="stylesheet">
  </head>
  <body>
    <div class="container">
          <%- include('header'); %>
          <h2> Clients </h2>
      <% for(var i = 0; i < clients.length; i++){ %>
            <li><%= i+1 %> | name : <%= clients[i].name %> | email : <%= clients[i].email %></li>
      <% } %><br>
  </div>
  </body>
</html>
